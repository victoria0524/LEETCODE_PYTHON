class Solution:
    # @param prices, a list of integer
    # @return an integer
    def maxProfit(self, prices):
        n=len(prices)
        if n<2:return 0
        low=prices[0]
        high=0
        
        for i in range (n):
            low=min(low,prices[i])
            high=max(high,prices[i]-low)
        
        return high
            
        
