class Solution:
    # @param    A       a list of integers
    # @param    elem    an integer, value need to be removed
    # @return an integer
    def removeElement(self, A, elem):
        n=len(A)
        i=0
        j=0
        
        while j<n:
            if A[j]!=elem:
                A[i]=A[j]
                i+=1
            j+=1
            
        return i
