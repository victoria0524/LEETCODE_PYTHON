class Solution:
    # @param S, a list of integer
    # @return a list of lists of integer
    def subsets(self, S):
        n=len(S)
        result=[]
        S.sort()
        self.helper(result,[],S,0)
        return result
        
    def helper(self,result,temp,S,step):
        result.append(temp[:])
        for i in range(step,len(S)):
            temp.append(S[i])
            self.helper(result,temp,S,i+1)
            temp.pop()
